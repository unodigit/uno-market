{
  "name": "ui-ux-build-plugin",
  "version": "1.0.1",
  "description": "Frontend UI/UX Build Plugin with parallel agent orchestration",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test",
    "build": "echo 'No build process required for Claude Code plugin'",
    "dev": "echo 'Use Claude Code to test plugin functionality'"
  },
  "keywords": [
    "claude-code",
    "plugin",
    "frontend",
    "ui",
    "ux",
    "react",
    "typescript",
    "tailwind",
    "vite",
    "automation",
    "quality",
    "agents"
  ],
  "author": {
    "name": "Development Team",
    "email": "dev@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ui-ux-build-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ui-ux-build-plugin/issues"
  },
  "homepage": "https://github.com/your-org/ui-ux-build-plugin#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@biomejs/biome": "^1.5.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-code": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/claude-code": {
      "optional": true
    }
  },
  "files": [
    ".claude-plugin/",
    "commands/",
    "agents/",
    "hooks/",
    "skills/",
    "scripts/",
    "config/",
    "README.md",
    "LICENSE"
  ]
}